{
  "name": "empleados-api",
  "dockerComposeFile": "./docker-compose.dev.yml",
  "service": "backend",
  "shutdownAction": "none",
  "customizations": {
    "vscode": {
      "extensions": ["esbenp.prettier-vscode", "dbaeumer.vscode-eslint", "Prisma.prisma"],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh"
      }
    }
  },
  "postCreateCommand": "sudo npm i npm -g && yarn && yarn build && yarn db:build",
  "workspaceFolder": "/app"
}
